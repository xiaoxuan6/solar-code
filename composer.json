{
    "name": "james.xue/solar-code",
    "description": "微信小程序生成太阳码",
    "type": "library",
    "keywords":
    [
        "solar-code",
        "code",
        "solar",
        "laravel"
    ],
    "homepage": "https://github.com/xiaoxuan6/solar-code.git",
    "license": "MIT",
    "authors": [
        {
            "name": "james.xue",
            "email": "1527736751@qq.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/cache": "^5.0|^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "James\\SolarCode\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "James\\SolarCode\\SolarCodeServiceProvider"
            ]
        },
        "aliases":{
            "SolarCode": "James\\SolarCode\\Facades\\SolarCode"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.7",
        "phpstan/phpstan": "^1.4",
        "laravel/framework": "^5.0|^6.0|^7.0|^8.0|^9.0"
    },
    "scripts": {
        "fix-style": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php --using-cache=no --ansi"
    }
}
